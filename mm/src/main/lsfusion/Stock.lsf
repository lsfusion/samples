//#header
//#module
MODULE Stock;
//#module end
//#header end

//#class
CLASS Stock 'Склад';

name 'Наименование' = DATA STRING[100] (Stock) IN base;
address 'Адрес' = DATA STRING[150] (Stock) IN base;
//#class end

//#form
FORM stocks 'Склады'
	OBJECTS s=Stock
	PROPERTIES(s) name, address, NEW, DELETE
;
//#form end